<resources>

    <!-- Base application theme. -->

    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="android:colorPrimary">@color/material_deep_teal_500</item>
        <item name="android:colorPrimaryDark">@color/material_deep_teal_500</item>
    </style>


    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="android:colorPrimary">@color/material_deep_teal_500</item>
        <item name="android:colorPrimaryDark">@color/material_deep_teal_500</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowContentTransitions">true</item>
        <item name="android:windowAllowEnterTransitionOverlap">true</item>
    </style>

    <style name="AppTheme.TransparentStatus" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:statusBarColor">@color/transparent</item>
        <item name="colorPrimaryDark">@color/material_deep_teal_500</item>
    </style>



</resources>
